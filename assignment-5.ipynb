   "source": [
    "#Assignment - 4 (Is it a Prime Number?)\n",
    "\n",
    "num = int(input(\"enter a number : \"))\n",
    "primelist = list()\n",
    "\n",
    "for i in range(2,num):\n",
    "    if num%i == 0:\n",
    "        primelist.append(i)\n",
    "if len(primelist)==0:\n",
    "    print(f'{num} is a prime number')\n",
    "else:\n",
    "    print(f'{num} is not a prime number')"
   ]
